		<![CDATA[SELECT ci.city,cu.first_name,cu.last_name,cu.email
FROM customer cu
INNER JOIN address a ON (cu.address_id= a.address_id)
INNER JOIN city ci ON (a.city_id=ci.city_id)]]>
